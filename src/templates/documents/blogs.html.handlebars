<!doctype html>
<html lang="en">
<head>
  {{> head }}
</head>
<body class="h-feed">
  {{> hcard-basics additionalClasses="p-author" }}
  <header>
    <h1 class="p-name"><a href="/">Qubyte Codes</a></h1>
    {{> nav }}
  </header>
  <main>
    {{#if blurb}}
    <p>{{{blurb}}}</p>
    {{/if}}
    {{#posts}}
    <article class="h-entry" light-emoji="{{lightEmoji}}" dark-emoji="{{darkEmoji}}">
      {{> blog-entry-header }}
      {{{snippet}}}
    </article>
    {{/posts}}
  </main>
  <footer>
    {{> copyright }}
  </footer>
</body>
</html>
