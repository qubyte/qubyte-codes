<!doctype html>
<html lang="en">
<head>
  {{> head }}
</head>
<body>
  <header>
    <h1><a href="/">Qubyte Codes</a></h1>
    {{> nav }}
  </header>
  <main>
    <article class="h-entry">
      {{> blog-entry-header }}
      <div class="e-content">
        {{{content}}}
      </div>
      {{> backlinks }}
      {{> webmentions }}
      <details class="share">
        <summary>Feel like sharing or responding?</summary>
        <p>{{> share-tweet callToAction="Tweet this post" }} or {{> share-toot callToAction="toot it!" }}</p>
        <p>Comments or corrections? {{> comments-tweet text="Send me a tweet" }} or {{> comments-toot text="toot to me!" }}</p>
        {{> webmention-form }}
        <p><a href="{{editUrl}}">Edit page.</a></p>
      </details>
    </article>
  </main>
  <footer>
    {{> copyright }}
  </footer>
</body>
</html>
