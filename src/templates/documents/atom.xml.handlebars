<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Qubyte Codes</title>
  <link href="https://qubyte.codes"/>
  <link type="application/atom+xml" rel="self" href="https://qubyte.codes/{{name}}.xml"/>
  <link href="https://paypal.me/qubyte" rel="payment" />
  <updated>{{isoDate updated}}</updated>
  <id>https://qubyte.codes/</id>
  <author>
    <name>qubyte</name>
  </author>

  {{#items}}
  <entry>
    <id>https://qubyte.codes{{localUrl}}</id>
    <link type="text/html" rel="alternate" href="https://qubyte.codes{{localUrl}}"/>
    <title>{{title}}</title>
    <published>{{isoDate datetime}}</published>
    <updated>{{#if updated}}{{isoDate updated}}{{else}}{{isoDate datetime}}{{/if}}</updated>
    <author>
      <name>qubyte</name>
      <uri>https://qubyte.codes</uri>
    </author>
    {{#if feedItem}}
    <content type="html">{{feedItem}}</content>
    {{else}}
    <content type="html">{{content}}</content>
    {{/if}}
  </entry>
  {{/items}}

</feed>
