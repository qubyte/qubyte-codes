{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Xss-Protection",
          "value": "1"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; style-src 'self'; img-src *; child-src https://www.youtube-nocookie.com 'self'; frame-src https://www.youtube-nocookie.com 'self';"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Feature-Policy",
          "value": "accelerometer 'self'; ambient-light-sensor 'self'; autoplay 'none'; camera 'self'; fullscreen 'self'; gyroscope 'self'; magnetometer 'self'; microphone 'self'; midi 'self'; picture-in-picture 'none'; sync-xhr 'none'; usb 'self';"
        }
      ]
    },
    {
      "source": "/japanese-notes*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "none"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=315360000, public, immutable"
        }
      ]
    },
    {
      "source": "/(.*).woff",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=315360000, public, immutable"
        }
      ]
    },
    {
      "source": "/(.*).woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=315360000, public, immutable"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "connect-src *;"
        }
      ]
    },
    {
      "source": "/atom.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/atom+xml; charset=utf-8"
        }
      ]
    }
  ]
}
